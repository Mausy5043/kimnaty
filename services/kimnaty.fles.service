[Unit]
Description=uWSGI instance to serve fles app
After=network.target

[Service]
User=pi
Group=www-data
WorkingDirectory=/home/pi/kimnaty/bin/
Environment="PATH=/bin:/usr/bin:/home/pi/kimnaty:/home/pi/kimnaty/bin/"
ExecStartPre=/home/pi/kimnaty/kimnaty --boot
ExecStart=/home/pi/.local/bin/uwsgi --socket 0.0.0.0:5000 --protocol=http --processes=1 -w wsgi:app

[Install]
WantedBy=multi-user.target
